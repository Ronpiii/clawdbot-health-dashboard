{
  "version": 2,
  "builds": [
    {
      "src": "api/*.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/system-metrics",
      "dest": "api/system-metrics.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
}